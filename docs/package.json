{
  "name": "docs",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.20.0pn",
  "description": "Documentation for relizy",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "license": "MIT",
  "scripts": {
    "build": "pnpm predocs && pnpm build:only",
    "build:only": "vitepress build",
    "dev": "pnpm predocs && vitepress dev",
    "ftp-deploy": "ftp-deploy --config ftp.json",
    "preview": "vitepress preview",
    "predocs": "typedoc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@maz-ui/icons": "^4.3.0",
    "@maz-ui/themes": "^4.3.0",
    "maz-ui": "^4.3.2",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@lehoczky/vue-termynal": "^1.3.0",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-vitepress-theme": "^1.1.2",
    "vitepress": "2.0.0-alpha.12",
    "vue-router": "^4.6.4",
    "vue-tsc": "^3.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,mts,cjs,md,yml,json}": [
      "eslint --fix"
    ]
  }
}
